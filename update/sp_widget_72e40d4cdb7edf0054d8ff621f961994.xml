        <client_script><![CDATA[function($scope) {
  /* widget controller */
  var c = this;
	$scope.applyAccount = function(){
		$('input:radio[name=typeApplicant]').filter('[value=intramural]').prop('checked', true);
		$('#action').val('add');
		$('#errMessage').hide();
		$('#savedId').val('');
		$('#isResubmit').val('submit');
		window.clearData();
		$('#permissionForm').modal('show');
	}
}]]></client_script>
